{
    "name": "84em/gravity-forms-ai",
    "description": "AI-powered analysis for Gravity Forms submissions using Claude API",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^2.0",
        "wp-phpunit/wp-phpunit": "^6.4",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "EightyFourEM\\GravityFormsAI\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EightyFourEM\\GravityFormsAI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage-report",
        "install-wp-tests": "./bin/install-wp-tests.sh wordpress_test root root localhost latest"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.4"
        }
    }
}